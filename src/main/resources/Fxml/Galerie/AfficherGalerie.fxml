        </GridPane>

        <Label fx:id="titreLabel" layoutX="287.0" layoutY="6.0" prefHeight="47.0" prefWidth="200.0" style="-fx-font-size: 24px;" text="Galeries" textAlignment="CENTER" />

        <Button fx:id="addG" layoutX="713.0" layoutY="14.0" mnemonicParsing="false" onAction="#addG" prefHeight="37.0" prefWidth="47.0">
