<AnchorPane fx:id="users_parent" prefHeight="569.0" prefWidth="775.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.controller.Galerie.AfficherGalerie">
    <children>
        <GridPane fx:id="galerieGrid" layoutX="38.0" layoutY="70.0" prefHeight="510.0" prefWidth="698.0">
